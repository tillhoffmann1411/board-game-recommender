{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    // running with this attributes: --input etl/data/test_bgg_games_1_100.csv --output etl/data/game_credits.csv  --batch-size 10 --delay 5
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "--input",
                "etl/data/test_bgg_games_1_100.csv",
                "--output",
                "etl/data/game_ratings_1_100.csv",
                "--batch-size",
                "10",
                "--delay",
                "2"
            ]
        }
    ]
}